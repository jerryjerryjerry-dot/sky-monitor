# Sky-Monitor 环境变量配置示例
# 复制此文件为 .env 并修改相应的值

# 数据库配置
# PostgreSQL
DB_HOST=localhost
DB_PORT=5432
DB_USERNAME=postgres
DB_PASSWORD=skypostgres
DB_DATABASE=sky_monitor

# ClickHouse
CLICKHOUSE_URL=http://localhost:8123
CLICKHOUSE_USERNAME=default
CLICKHOUSE_PASSWORD=skyclickhouse

# Redis
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=skyredis
REDIS_DB=0

# Kafka
KAFKA_BROKERS=localhost:9092
KAFKA_CLIENT_ID=sky-monitor
KAFKA_GROUP_ID=sky-monitor-consumer

# 服务端口
# Monitor Server (API Server)
MONITOR_SERVER_PORT=8081

# DSN Server (SDK Server)
DSN_SERVER_PORT=8080

# Frontend Dev Server
VITE_PORT=5173

# JWT 配置
JWT_SECRET=your-super-secret-key-change-in-production
JWT_EXPIRES_IN=7d

# Node 环境
NODE_ENV=development
